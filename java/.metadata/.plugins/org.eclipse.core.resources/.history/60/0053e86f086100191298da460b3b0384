package base;

public class KouJue {
	public static void main(String args[]) {
		//int a[][]= {{1,2,3,4,5,6,7,8,9},{1,2,3,4,5,6,7,8,9}};
		String[][] b=new String[9][0];
		for(int i=0;i<9;i++) {
			b[i]=new String[i+1];
			for(int j=0;j<=i;j++) {
				b[i][j]=j+"*"+i+"="+i*j;
			}
		}
		
		for(int i=0;i<9;i++) {
			System.out.print(b[i]);
		}
		System.out.println("");
	}
}
